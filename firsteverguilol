Section:NewToggle("HidePets", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
        -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Hide Pets",
    [2] = true
}

game:GetService("ReplicatedStorage").Events.SetOption:FireServer(unpack(args))

    else
        print("Toggle Off")
        -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Hide Pets",
    [2] = false
}

game:GetService("ReplicatedStorage").Events.SetOption:FireServer(unpack(args))

    end
end)


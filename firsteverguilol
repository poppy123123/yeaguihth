Section:NewToggle("Auto Hatch Crystal Egg", "ToggleInfo", function(state)
    if state then
        while true do
            wait(3)
            local args = {
                [1] = "Crystal Egg",
                [2] = true
            }
            
            game:GetService("ReplicatedStorage").Events.OpenEgg:FireServer(unpack(args))
        end
    else
        local args = {
            [1] = "Crystal Egg",
            [2] = false
        }
        
        game:GetService("ReplicatedStorage").Events.OpenEgg:FireServer(unpack(args))
    end
end)

